	at sources.State.getMinMax(State.java:1427)
	at sources.State.getMinMax(State.java:1325)
	at sources.Pruebas.main(Pruebas.java:68)

// He añadido un try catch en el main para solucionar el problema de memoriaque seguía existiendo

	try {
		min_max = state.getMinMax(n);
	} 
	catch(OutOfMemoryError e) {
		min_max = state.getMinMax(n - 1);
	}			

