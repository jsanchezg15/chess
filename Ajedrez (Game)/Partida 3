Exception in thread "main" java.lang.OutOfMemoryError: Java heap space
	at sources.State.clone_board(State.java:1266)
	at sources.State.rook_movement(State.java:763)
	at sources.State.next_states(State.java:202)
	at sources.State.getMinMax(State.java:1351)
	at sources.State.getMinMax(State.java:1422)
	at sources.State.getMinMax(State.java:1400)
	at sources.State.getMinMax(State.java:1422)
	at sources.State.getMinMax(State.java:1320)
	at sources.Pruebas.main(Pruebas.java:59)
	
// Añadiendo el trimToSize a esta función parece que se ha arreglado y ya no hay problemas con falta de memoria

	private Vector<Vector<String>> clone_board() {
			Vector<Vector<String>> clone = new Vector<Vector<String>>();
			
			for(int i = 0; i < 8; i++) {
				clone.add(new Vector<String>());
				for(int j = 0; j < 8; j++)
					clone.get(i).add(this.board.get(i).get(j));
			}
			
			for(int i = 0; i < 8; i++) 
				clone.get(i).trimToSize();
			
			clone.trimToSize();
			
			return clone;		
		}
		
